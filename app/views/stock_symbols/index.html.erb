<h1>Listing stock_symbols</h1>

<table>
  <tr>
  </tr>

<% @stock_symbols.each do |stock_symbol| %>
  <tr>
    <td><%= link_to 'Show', stock_symbol %></td>
    <td><%= link_to 'Edit', edit_stock_symbol_path(stock_symbol) %></td>
    <td><%= link_to 'Destroy', stock_symbol, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New stock_symbol', new_stock_symbol_path %>